{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "ag",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "ag",
      "kebab-case"
    ]
  },
  "experimentalDecorators": true,
  "no-duplicate-variable": true,
  "no-any": false,
  "no-consecutive-blank-lines": true,
  "no-inferrable-types": false
}
